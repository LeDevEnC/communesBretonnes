<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControllerDataSee">
   <center>
      <TableView fx:id="tableView" BorderPane.alignment="CENTER">
         <columns>
            <TableColumn fx:id="colScore" text="Score">
               <cellValueFactory>
                  <PropertyValueFactory property="score" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colVille" text="Ville">
               <cellValueFactory>
                  <PropertyValueFactory property="ville" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colCodePostal" text="Code Postal">
               <cellValueFactory>
                  <PropertyValueFactory property="codePostal" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colDepartement" text="DÃ©partement">
               <cellValueFactory>
                  <PropertyValueFactory property="departement" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colINSEE" text="Code INSEE">
               <cellValueFactory>
                  <PropertyValueFactory property="insee" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colPrix" text="Prix">
               <cellValueFactory>
                  <PropertyValueFactory property="prix" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colNb" text="Nb">
               <cellValueFactory>
                  <PropertyValueFactory property="nb" />
               </cellValueFactory>
            </TableColumn>
         </columns>
         <BorderPane.margin>
            <Insets left="10.0" right="10.0" />
         </BorderPane.margin>
      </TableView>
   </center>
   <top>
      <TextField promptText="Rechercher ..." BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </TextField>
   </top>
   <bottom>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button styleClass="nav-button" text="Previous" />
            <Button styleClass="page-button, selected" text="1" />
            <Button styleClass="page-button" text="2" />
            <Button styleClass="page-button" text="3" />
            <Button styleClass="nav-button" text="Next" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
